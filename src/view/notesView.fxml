<fx:root type="javafx.scene.control.TitledPane" fx:id="view" alignment="TOP_LEFT" animated="false" collapsible="false"
         contentDisplay="TEXT_ONLY" expanded="true" focusTraversable="false" graphicTextGap="0.0" mouseTransparent="false"
         prefHeight="562.0" prefWidth="543.0" text="Цирк им. Игоря Стечкина" textAlignment="LEFT" textOverrun="ELLIPSIS"
         wrapText="true" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2"
         fx:controller="controller.NotesController">
  <content>
    <AnchorPane id="Content" maxHeight="-1.0" maxWidth="-1.0" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
      <children>
        <ListView fx:id="notesListView" focusTraversable="false" maxHeight="-1.0" maxWidth="-1.0" prefHeight="497.0" prefWidth="-1"
                  style="" AnchorPane.bottomAnchor="35.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                  AnchorPane.topAnchor="0.0">
          <stylesheets>
            <URL value="@main.css" />
          </stylesheets>
        </ListView>
        <ToolBar prefWidth="235.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
          <items>
            <Button fx:id="addButton" cancelButton="false" defaultButton="false" focusTraversable="false" graphicTextGap="1.0" mnemonicParsing="false" onAction="#handleAddButtonAction" text="Добавить" />
