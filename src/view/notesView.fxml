            <Button fx:id="removeButton" focusTraversable="false" mnemonicParsing="false" onAction="#handleRemoveButtonAction" text="Удалить" />
          </items>
        </ToolBar>
      </children>
    </AnchorPane>
  </content>
  <font>
    <Font name="System Bold" size="15.0" />
  </font>
</fx:root>
